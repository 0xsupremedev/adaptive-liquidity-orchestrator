# Environment Variables - Copy to .env and fill in values

# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# opBNB RPC endpoints (defaults work, but you can use custom)
OPBNB_TESTNET_RPC=https://opbnb-testnet-rpc.bnbchain.org
OPBNB_MAINNET_RPC=https://opbnb-mainnet-rpc.bnbchain.org

# opBNBScan API key for contract verification
OPBNBSCAN_API_KEY=your_api_key_here

# Backend configuration
DATABASE_URL=file:./dev.db
JWT_SECRET=your_jwt_secret_here
PORT=3001

# AI Optimizer configuration
VOLATILITY_THRESHOLD=0.05
SLIPPAGE_THRESHOLD=0.02
REBALANCE_COOLDOWN=3600
